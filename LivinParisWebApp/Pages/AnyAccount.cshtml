@page
@model LivinParisWebApp.Pages.AnyAccountModel
@{
    Layout = "~/Pages/Shared/_NoAccountLayout.cshtml";
    ViewData["Title"] = "Pas de compte";
}
<div class="popup-content">
    <div class="popup-no-account">
        <p class="title-popup">Vous n'avez aucun espace !</p>
        <div class="popup-question">
            <div>
                <p class="popup-question-text">Voulez vous en créer un ?</p>
                <form method="post">
                    <button class="btn-yes"
                            type="submit"
                            asp-page-handler="Register">
                        <p>Oui</p>
                    </button>
                </form>
            </div>
        </div>
    </div>
    @* rediriger vers panel client *@
    <form method="post">
        <button class="btn-retour" type="submit" asp-page-handler="Login">Retour</button>
    </form>
</div>